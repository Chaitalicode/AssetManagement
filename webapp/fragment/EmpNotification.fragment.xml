<core:FragmentDefinition xmlns="sap.m"	xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"  xmlns:viz="sap.viz.ui5.controls"  xmlns:viz.data="sap.viz.ui5.data"  xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"	xmlns:f="sap.ui.layout.form">
	<ResponsivePopover 
		title="Name"
		class="sapUiPopupWithPadding"
		placement="Bottom" showCloseButton="true" afterClose="onAfterClose" >
		<VBox >
	<List id="EmpList" items="{main>/EmpFrag}" showNoData="false">
	    <NotificationListItem
				title="Your request for {main>nwItem} has been {main>status}"
				showCloseButton="true"
				unread="true"
				priority="None"
				counter="1"
				
				close=".onItemClose"
				press=".onListItemPress"
				>
			
                
		</NotificationListItem>
	</List>
	<List id="EmpList2" items="{main>/EmpFrag2}" showNoData="false">
	    <NotificationListItem
				title="Your request for {main>asstNm} has been {main>status}"
				showCloseButton="true"
				unread="true"
				priority="None"
				counter="1"
				
				close=".onItemClose"
				press=".onListItemPress"
				>
				
                
		</NotificationListItem>
	</List>
	</VBox> 
    </ResponsivePopover>
</core:FragmentDefinition>