<core:FragmentDefinition xmlns="sap.m"  xmlns:ui="sap.ui.table"	xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" 	xmlns:f="sap.ui.layout.form">
<!-- <Dialog id="idDialogS67" showHeader="true" title="Request Form" > -->

<!-- New Assets -->
	<IconTabBar
		id="idIconTabBar"
		select=".onFilterSelect"
		class="sapUiResponsiveContentPadding">
        	<items>
            <IconTabFilter	icon="sap-icon://begin"	iconColor="Positive" text="New Assets Requests"	key="New" >
            <Table id="empReqFrNew"
		inset="false"
		items="{
			path: 'main>/NewReqAsst'
		}">
		<headerToolbar>
			<OverflowToolbar>
				<content>
					<Title text="New Assets" level="H2"/>
					<ToolbarSpacer />
				</content>
			</OverflowToolbar>
		</headerToolbar>
		<infoToolbar>
			<OverflowToolbar>
				<Label text="Requests for New Assests"/>
			</OverflowToolbar>
		</infoToolbar>
		<columns>
			<Column
				width="12em">
				<Text text="Employee Id" />
			</Column>
            <Column
				width="12em">
				<Text text="Employee Name" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Requested Date" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Requested Assest" />
			</Column>
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<Text text="Status" />
			</Column>
            <Column
				hAlign="End">
				<Text text="Reason" />
			</Column>

			 <Column
				hAlign="End">
				<Text text="Accept" />
			</Column>

			 <Column
				hAlign="End">
				<Text text="Reject" />
			</Column>
		
		</columns>
		<items>
			<ColumnListItem>
				<cells>
                    <Text
						text="{main>empID}" />
                    <Text
						text="{main>name}" />
                    <Text
						text="{main>empDate}" />
					<Text
						text="{main>nwItem}" />
					<Text
						text="{main>status}" />
                    <Text
						text="{main>reason}" />
					 <Button
						text="Accept" press="onAcceptNew" />
					 <Button
						text="Reject" press="onRejectNew" />	
				
				</cells>
			</ColumnListItem>
		</items>
	</Table> 
             </IconTabFilter>
			 <!--  -->

			 <!-- Old Assets -->
			<IconTabFilter
				icon="sap-icon://compare"
				iconColor="Critical"
				text="Old Assets Requests"
				key="Old" >

				 <Table id="empReqFrOld"
		inset="false"
		items="{
			path: 'main>/OldAssestReq'
		}">
		<headerToolbar>
			<OverflowToolbar>
				<content>
					<Title text="Old Assets" level="H2"/>
					<ToolbarSpacer />
				</content>
			</OverflowToolbar>
		</headerToolbar>
		<infoToolbar>
			<OverflowToolbar>
				<Label text="Requests for Old Assests"/>
			</OverflowToolbar>
		</infoToolbar>
		<columns>
			<Column
				width="12em">
				<Text text="Employee Id" />
			</Column>
            <Column
				width="12em">
				<Text text="Employee Name" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Assest ID" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Assest Name" />
			</Column>
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<Text text="Status" />
			</Column>
            <Column
				hAlign="End">
				<Text text="Reason" />
			</Column>
		 <Column
				hAlign="End">
				<Text text="Attachments" />
			</Column>
			 <Column
				hAlign="End">
				<Text text="Accept" />
			</Column>
		 <Column
				hAlign="End">
				<Text text="Reject" />
			</Column>
		
		</columns>
		<items>
			<ColumnListItem>
				<cells>
                    <Text
						text="{main>empId}" />
                    <Text
						text="{main>empName}" />
                    <Text
						text="{main>asstId}" />
					<Text
						text="{main>asstNm}" />
					<Text
						text="{main>status}" />
                    <Text
						text="{main>reason}" />	
					
				<Link href="{main>img}" target="_blank" text="Download"/>
				<Button
						text="Accept" press="onOldAcceptPress" />
					 <Button
						text="Reject" press="onOldRejectPress" />	
				
				</cells>
			</ColumnListItem>
		</items>
	</Table> 
	</IconTabFilter>
	
            	</items>
        	</IconTabBar>
	
